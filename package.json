{
	"name": "@douganderson444/svelte-solana-name-service",
	"version": "0.0.7",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"svelte": "src/lib/index.js",
	"scripts": {
		"dev": "svelte-kit dev --port 3113",
		"build": "svelte-kit build",
		"build:rollup": "rollup -c",
		"build:solana:web3": "esbuild ./node_modules/@solana/web3.js --bundle --format=esm --main-fields=browser,module,main --define:process.env.NODE_ENV='\"production\"' --splitting --outdir=./src/modules/@solana/web3.js",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@rollup/plugin-alias": "^3.1.9",
		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@rollup/plugin-typescript": "^8.3.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"crypto-browserify": "^3.12.0",
		"esbuild": "^0.14.15",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"globby": "^13.1.0",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"rollup-plugin-css-only": "^3.1.0",
		"rollup-plugin-inline-svg": "^2.0.0",
		"rollup-plugin-node-builtins": "^2.1.2",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.2",
		"svelte2tsx": "^0.5.1",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"type": "module",
	"dependencies": {
		"@originjs/vite-plugin-commonjs": "^1.0.2",
		"@solana/web3.js": "^1.32.0",
		"borsh": "^0.7.0",
		"buffer": "^6.0.3",
		"hash.js": "^1.1.7",
		"path": "^0.12.7",
		"process": "^0.11.10",
		"url": "^0.11.0",
		"util": "^0.12.4"
	},
	"repository": "https://github.com/DougAnderson444/svelte-solana-name-service"
}
